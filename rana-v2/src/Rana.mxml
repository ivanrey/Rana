		<s:State enterState="timerSiguienteTurno()" name="Blanqueado"/>
		<s:State name="FinJuego"/>
		<s:State enterState="timerSiguienteTurno()" name="Ganaste"/>
		<s:State name="Video"/>
	</s:states>
	
